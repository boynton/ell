;;
;; changes to be able to compile scheme code
;;
;; ell has null (like nil), distinct from empty list '(). Scheme uses null? to check for empty list
;;

(define (null? x) (equal? x '()))
(define (pair? x) (and (list? x) (not (empty? x))))

(define make-vector make-array)
(define vector array)
(define vector? array?)
(define vector-ref array-ref)
(define vector-set! array-set!)
